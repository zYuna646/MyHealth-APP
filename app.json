{
  "expo": {
    "name": "MyHealth",
    "slug": "MyHealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon_heart.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/Home/heart.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow Voice to Text Tutorial to access the microphone",
          "speechRecognitionPermission": "Allow Voice to Text Tutorial to securely recognize user speech"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/icon_heart.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zyuna646.MyHealth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0209858f-5041-4fee-9c08-67904648abbd"
      }
    }
  }
}
